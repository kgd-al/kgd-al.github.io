title = {{Connectionist Theory Refinement: Genetically Searching the Space of Network Topologies}},
url = {https://www.jair.org/index.php/jair/article/view/10186 https://jair.org/index.php/jair/article/view/10186},
volume = {6},
year = {1997}
}
