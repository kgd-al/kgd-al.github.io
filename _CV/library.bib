pages = {1--73},
title = {{A Systematic Literature Review of the Successors of “NeuroEvolution of Augmenting Topologies”}},
url = {https://direct.mit.edu/evco/article/29/1/1-73/97341},
volume = {29},
year = {2021}
}
@article{Majid2021,
