keywords = {Biodiversity,Biological Evolution,Plants,Plants: classification,Plants: genetics},
number = {2},
pages = {199--222},
pmid = {22356151},
title = {{Emergent Diversity in an Open-Ended Evolving Virtual Community}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22356151 http://www.mitpressjournals.org/doi/10.1162/artl{\_}a{\_}00059},
