arxivId = {10.2307/2251299},
author = {Turing, A M},
doi = {10.1038/145662c0},
eprint = {2251299},
file = {:home/kgd/work/mendeley/Turing/1950 - Nature - Psychology and Philosophy.pdf:pdf},
isbn = {9781402085819},
issn = {0028-0836},
