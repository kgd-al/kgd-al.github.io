booktitle = {The 2019 Conference on Artificial Life},
doi = {10.1162/isal_a_00145.xml},
file = {:home/kgd/work/mendeley/Campos, Froese/2019 - The 2019 Conference on Artificial Life - From embodied interaction to compositional referential communication A minimal agent-bas.pdf:pdf},
isbn = {978-0-262-35844-6},
pages = {79--86},
