isbn = {978-0-262-33936-0},
keywords = {predator confusion effect,predator-prey coevolution,swarming behavior,visual acuity},
pages = {250--257},
publisher = {MIT Press},
title = {{Exploring the coevolution of predator and prey morphology and behavior}},
url = {http://arxiv.org/abs/1602.08802 https://www.mitpressjournals.org/doi/abs/10.1162/978-0-262-33936-0-ch045},
